                <mxCell id="variant_utils" value="ðŸ”§ Variant Utilities&#xa;src/theme/utils.ts&#xa;&#xa;import { clsx } from &#39;clsx&#39;;&#xa;import { twMerge } from &#39;tailwind-merge&#39;;&#xa;&#xa;export const cn = (...classes) =&gt;&#xa;  twMerge(clsx(classes));&#xa;&#xa;export const cva = (base, config) =&gt; {&#xa;  return (props) =&gt; {&#xa;    const { variant, size, ...rest } = props;&#xa;    return cn(&#xa;      base,&#xa;      config.variants?.[variant],&#xa;      config.sizes?.[size],&#xa;      rest.className&#xa;    );&#xa;  };&#xa;};" style="whiteSpace=wrap;html=1;fillColor=#d5e8d4;strokeColor=#82b366;align=left;verticalAlign=top;fontSize=10;fontFamily=Courier New;" parent="theme_foundation" vertex="1">
