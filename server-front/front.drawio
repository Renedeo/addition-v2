                <mxCell id="button_component" value="ðŸ“¦ Button.tsx&#xa;&#xa;import { forwardRef } from &#39;react&#39;;&#xa;import { useButton } from &#39;./useButton&#39;;&#xa;import type { IButton } from &#39;./interfaces&#39;;&#xa;&#xa;export const Button = forwardRef&lt;&#xa;  HTMLButtonElement,&#xa;  IButton&#xa;&gt;(({ children, leftIcon, rightIcon, ...props }, ref) =&gt; {&#xa;  const { className, disabled } = useButton(props);&#xa;&#xa;  return (&#xa;    &lt;button&#xa;      ref={ref}&#xa;      className={className}&#xa;      disabled={disabled}&#xa;      {...props}&#xa;    &gt;&#xa;      {leftIcon}&#xa;      {props.loading ? &lt;Spinner /&gt; : children}&#xa;      {rightIcon}&#xa;    &lt;/button&gt;&#xa;  );&#xa;});" style="whiteSpace=wrap;html=1;fillColor=#d5e8d4;strokeColor=#82b366;align=left;verticalAlign=top;fontSize=10;fontFamily=Courier New;" parent="button_module" vertex="1">
